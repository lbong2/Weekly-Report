{
  "name": "weekly-report",
  "version": "1.0.0",
  "private": true,
  "description": "SM 프로젝트 주간보고서 시스템",
  "scripts": {
    "dev": "turbo run dev",
    "dev:backend": "pnpm --filter backend dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "build": "turbo run build",
    "build:backend": "pnpm --filter backend build",
    "build:frontend": "pnpm --filter frontend build",
    "db:migrate": "pnpm --filter backend prisma:migrate",
    "db:migrate:dev": "pnpm --filter backend prisma:migrate:dev",
    "db:seed": "pnpm --filter backend prisma:seed",
    "db:studio": "pnpm --filter backend prisma:studio",
    "db:reset": "pnpm --filter backend prisma:reset"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.4"
}
